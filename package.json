    "@types/babel-core": "^6.25.6",
    "@types/babel-types": "^7.0.7",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-plugin-tester": "^6.4.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
